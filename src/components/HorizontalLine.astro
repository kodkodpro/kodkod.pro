---
type Props = {
  label: string
  class?: string
}

const { label, class: className }: Props = Astro.props
---

<div class:list={["flex items-center gap-x-2", className]}>
  <div class="border-b flex-1" />
  <div class="text-gray-400 text-xs font-semibold uppercase">{label}</div>
  <div class="border-b flex-1" />
</div>